{
  "name": "nuxt",
  "private": false,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxt build --analyze",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue",
    "lint:js:fix": "eslint --fix --ext .js,.vue",
    "lint:css": "stylelint '**/*.vue' --custom-syntax postcss-html",
    "lint:css:fix": "stylelint '**/*.vue' --custom-syntax postcss-html --fix",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.6",
    "nuxt": "^3.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@vue/test-utils": "^2.2.5",
    "@vue/vue3-jest": "^29.2.1",
    "babel-core": "7.0.0-bridge.0",
    "eslint": "^8.29.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "jest": "29.3.1",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "sass": "^1.56.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-strict-value": "^1.9.1"
  }
}
